<rxui:ReactiveWindow x:Class="MusicMachine.Controls.MainControl.MainView"
        x:TypeArguments="vm:MainViewModel"
        xmlns:rxui="http://reactiveui.net"
        xmlns:vm="clr-namespace:MusicMachine.Controls.MainControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusicMachine.Controls.SequenzerElements.MultiSequenzer"
        mc:Ignorable="d"
        Title="{Binding MultiSequenzerViewModel.CurrentProjectName}" Height="600" Width="1155" Icon="pack://application:,,,/MusicMachine;component/Resources/Note.png">
    <Grid>
        <local:MultiSequenzerView DataContext="{Binding MultiSequenzerViewModel}"/>
    </Grid>
</rxui:ReactiveWindow>
